{
  "bundler-config": "build.rsruby --with-R-dir=/usr/lib/R --with-R-include=/usr/share/R/include",
  "group": "nogroup",
  "server_roles": [
    "mroi"
  ],
  "repository": "git://github.com/newtonlabs/mroi.git",
  "force": {
    "production": false
  },
  "migrate": {
    "production": true
  },
  "packages": {
    "libcurl4-openssl-dev": "",
    "libxslt-dev": "",
    "libxml2-dev": ""
  },
  "databases": {
    "production": {
      "reconnect": "true",
      "encoding": "utf8",
      "username": "mroi",
      "adapter": "mysql",
      "database": "mroi",
      "password": "awesome_password"
    }
  },
  "id": "mroi",
  "mysql_repl_password": {
    "production": "mysql_repl"
  },
  "type": {
    "mroi": [
      "rails",
      "mck_passenger"
    ]
  },
  "database_master_role": [
    "mroi_database_master"
  ],
  "migration_command": "rake db:migrate",
  "revision": {
    "production": "master"
  },
  "gems": {
    "rake": "",
    "bundler": ""
  },
  "mysql_root_password": {
    "production": "mysql_root"
  },
  "mysql_debian_password": {
    "production": "mysql_debian"
  },
  "deploy_to": "/srv/mroi",
  "owner": "nobody"
}
